{
	"name":           "scurry",
	"version":        "0.0.2",
	"description":    "A leveldb-backed consistent hash ring, for your toy caching needs.",
	"author":         "C J Silverio <ceejceej@gmail.com>",
	"license":        "MIT",
	"readmeFilename": "README.md",
	"main":           "index.js",
	"scripts":
	{
		"start": "node index.js",
		"test":  "mocha -R spec test/test*.js",
		"node1": "node index.js --id=node-one -p 3332 -g 4114 -m -d ./db | ./node_modules/.bin/bunyan -o short",
		"node2": "node index.js --id=node-two -p 3333 -g 4114 -s 10.0.0.5 -d ./db2 | ./node_modules/.bin/bunyan -o short",
		"node3": "node index.js --id=node-three -p 3334 -g 4114 -s 10.0.0.5 -d ./db3 | ./node_modules/.bin/bunyan -o short"
	},
	"repository":
	{
		"type": "git",
		"url":  "https://github.com/ceejbot/scurry"
	},
	"keywords":
	[
		"leveldb",
		"scuttlebutt"
	],
	"gitHead":        "8b34720988b12cfb98a7c9c406f0e80e7d43974c",
	"bugs":
	{
		"url": "https://github.com/ceejbot/scurry/issues"
	},
	"dependencies":
	{
		"bunyan":         "*",
		"crdt":           "*",
		"level-sublevel": "*",
		"level-ttl":      "*",
		"leveldown":      "*",
		"levelup":        "*",
		"light-cycle":    "*",
		"my-local-ip":    "*",
		"node-uuid":      "*",
		"optimist":       "*",
		"p-promise":      "*",
		"restify":        "*",
		"scuttlebutt":    "*"
	}
}
