{
  "name": "scurry",
  "description": "A leveldb-backed consistent hash ring, for your toy caching needs.",
  "version": "0.0.10",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": {
    "url": "https://github.com/ceejbot/scurry/issues"
  },
  "dependencies": {
    "bistre": "~1.0.1",
    "bole": "~3.0.2",
    "crdt": "~3.6",
    "level-sublevel": "~6.6.2",
    "level-ttl": "~3.1.0",
    "leveldown": "~4.0.1",
    "levelup": "~3.0.1",
    "light-cycle": "~1.4.3",
    "lodash": "~4.17.10",
    "my-local-ip": "*",
    "p-promise": "~0.5.0",
    "restify": "~7.2.1",
    "scuttlebutt": "~5.6.8",
    "uuid": "~3.2.1",
    "yargs": "~11.0.0"
  },
  "devDependencies": {
    "coveralls": "~3.0.1",
    "eslint-config-ceejbot": "~1.1.2",
    "mocha": "~5.2.0",
    "must": "~0.13.1",
    "nyc": "~12.0.2",
    "rimraf": "~2.6.2",
    "xo": "~0.21.1"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "homepage": "https://github.com/ceejbot/scurry#readme",
  "keywords": [
    "autosharding",
    "leveldb",
    "scuttlebutt"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceejbot/scurry.git"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "xo",
    "node1": "node index.js -c ./examples/server.json | bistre",
    "node2": "node index.js -c ./examples/node-two.json | bistre",
    "node3": "node index.js -c ./examples/node-three.json | bistre",
    "start": "node index.js --id=server -m -g 4114 -p 3333 --dbpath=./db | bistre",
    "test": "nyc mocha -R spec",
    "travis": "npm test && npm run coverage"
  },
  "xo": {
    "extends": "eslint-config-ceejbot",
    "rules": {
      "unicorn/no-process-exit": 0,
      "prefer-destructuring": 0
    }
  }
}
